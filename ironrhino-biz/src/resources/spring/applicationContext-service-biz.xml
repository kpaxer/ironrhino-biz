<?xml version="1.0" encoding="UTF-8"?>
<beans default-autowire="byName"
	xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="userManager" class="com.ironrhino.biz.service.UserManagerImpl" />
	<bean id="productManager" class="com.ironrhino.biz.service.ProductManagerImpl" />
	<bean id="customerManager" class="com.ironrhino.biz.service.CustomerManagerImpl" />
	<bean id="orderManager" class="com.ironrhino.biz.service.OrderManagerImpl" />
	<bean id="categoryTreeControl" class="com.ironrhino.biz.support.CategoryTreeControl" />
	<bean id="regionTreeControl" class="com.ironrhino.biz.support.RegionTreeControl" />
</beans>
