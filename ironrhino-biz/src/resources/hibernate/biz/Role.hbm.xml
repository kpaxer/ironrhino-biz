<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.biz.model"
	default-lazy="true">
	<class name="Role" table="role">
		<id name="id" type="java.lang.String" length="22"
			unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<natural-id mutable="true">
			<property name="name" not-null="true" />
		</natural-id>
		<set name="users" table="user_role" cascade="none"
			inverse="false" lazy="true">
			<key column="roleId" />
			<many-to-many class="User" column="userId" />
		</set>
	</class>
</hibernate-mapping>