<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.biz.model"
	default-lazy="true">
	<class name="Stuffflow" table="stuffflow"
		dynamic-insert="true" dynamic-update="true">
		<id name="id" type="java.lang.String"  length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<property name="quantity" />
		<property name="amount" />
		<property name="requestDate" />
		<property name="auditDate" />
		<property name="status">
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">org.ironrhino.common.model.Status</param>
				<!-- if set type=12 then use name not ordinal
					<param name="type">12</param>
					-->
			</type>
		</property>
		<many-to-one name="requestUser" column="requestUserId" fetch="join"/>
		<many-to-one name="auditUser" column="auditUserId" fetch="join"/>
		<many-to-one name="stuff" column="stuffId" not-null="true" fetch="join"/>
	</class>
</hibernate-mapping>