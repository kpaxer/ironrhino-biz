<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.biz.model"
	default-lazy="true">
	<class name="Stuffflow" table="stuffflow" dynamic-insert="true"
		dynamic-update="true">
		<id name="id" type="java.lang.String" length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<property name="quantity" />
		<property name="amount" />
		<property name="date" />
		<property name="memo" length="2500" />
		<property name="createDate" />
		<many-to-one name="stuff" column="stuffId" not-null="true"
			fetch="join"  foreign-key="none"/>
	</class>
</hibernate-mapping>