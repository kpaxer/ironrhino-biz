<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.biz.model"
	default-lazy="true">
	<class name="Spec" table="spec">
		<id name="id" type="java.lang.String" length="32"
			unsaved-value="null">
			<generator class="uuid" />
		</id>
		<property name="basicPackName" not-null="true" />
		<property name="basicQuantity"/>
		<property name="basicMeasure"/>
		<property name="baleQuantity"/>
		<property name="balePackName"/>
		<property name="purity"/>
	</class>
</hibernate-mapping>