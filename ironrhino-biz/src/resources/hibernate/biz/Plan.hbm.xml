<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.biz.model"
	default-lazy="true">
	<class name="Plan" table="plan" dynamic-insert="true"
		dynamic-update="true">
		<id name="id" type="java.lang.String" length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<property name="quantity" />
		<property name="completed" />
		<property name="planDate" />
		<property name="completeDate" />
		<property name="createDate" />
		<property name="memo" length="2500" />
		<many-to-one name="product" column="productId" not-null="true"
			lazy="false" fetch="join" foreign-key="none" />
	</class>
</hibernate-mapping>