<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.biz.model"
	default-lazy="true">
	<class name="Returning" table="`returning`" dynamic-insert="true"
		dynamic-update="true">
		<id name="id" type="java.lang.String" length="32" unsaved-value="null">
			<generator class="uuid" />
		</id>
		<version name="version" column="version" access="field" />
		<property name="freight" />
		<property name="grandTotal" />
		<property name="returnDate" />
		<property name="createDate" />
		<property name="modifyDate" />
		<property name="memo" length="2500" />
		<many-to-one name="customer" column="customerId" not-null="true"
			lazy="false" fetch="join" />
		<many-to-one name="station" column="stationId" lazy="proxy" />
		<many-to-one name="salesman" column="salesmanId" lazy="proxy" />
		<many-to-one name="createUser" lazy="proxy" not-found="ignore" />
		<many-to-one name="modifyUser" lazy="proxy" not-found="ignore" />
		<list name="items" table="returningitem" cascade="all" lazy="true"
			fetch="subselect">
			<key column="returningId" />
			<list-index column="lineNumber" />
			<composite-element class="OrderItem">
				<many-to-one name="product" column="productId" not-null="true"
					lazy="false" fetch="join" />
				<property name="quantity" />
				<property name="price" />
			</composite-element>
		</list>
	</class>
</hibernate-mapping>