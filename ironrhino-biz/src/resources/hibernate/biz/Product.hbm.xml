<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.biz.model"
	default-lazy="true">
	<class name="Product" table="product" dynamic-insert="true"
		dynamic-update="true">
		<id name="id" type="java.lang.String"  length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<natural-id>
			<property name="name" not-null="true" />
			<many-to-one name="spec" column="specId" />
		</natural-id>
		<property name="stock" />
		<property name="criticalStock" />
		<many-to-one name="category" column="categoryId" />
	</class>
</hibernate-mapping>