<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.biz.model"
	default-lazy="true">
	<class name="Customer" table="customer" dynamic-insert="true"
		dynamic-update="true">
		<id name="id" type="java.lang.Long" length="32" unsaved-value="null">
			<generator class="identity" />
		</id>
		<property name="name" not-null="true" />
		<property name="address" />
		<property name="linkman" />
		<property name="phone" />
		<property name="fax" />
		<property name="memo" />
		<property name="createDate" />
		<many-to-one name="region" column="regionId" lazy="false"
			fetch="join" />
	</class>
</hibernate-mapping>